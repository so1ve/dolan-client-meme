    "eslint": "^8.32.0",
    "fs-extra": "^11.1.0",
    "gitalk": "^1.8.0",
    "husky": "^8.0.3",
    "mini-cz": "^0.5.0",
    "nuxt": "3.4.3",
    "nuxt-purgecss": "^2.0.0",
    "pinia": "^2.0.29",
    "qrcode.vue": "^3.3.3",
    "sass": "^1.57.1",
    "shiki-es": "^0.2.0",
    "stylelint": "^14.16.1",
    "ts-json-schema-generator": "^1.2.0",
    "tsx": "^3.12.2",
    "twikoo": "^1.6.8",
    "typescript": "^4.9.4",
    "unplugin-icons": "^0.15.1",
    "vite-plugin-toml": "^0.5.0",
    "vue-utterances": "^0.3.4"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "*"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "unplugin",
        "vite",
        "vue",
        "webpack",
        "postcss",
        "rollup",
        "unified",
