{
  "name": "dolan-client-volantis",
  "version": "0.0.0",
  "packageManager": "pnpm@7.20.0",
  "main": "nuxt.config.ts",
  "files": [
    "src",
    "nuxt.config.ts"
  ],
  "scripts": {
    "build": "nuxi build",
    "build:copy-shiki-assets": "tsx scripts/copy-shiki-assets.ts",
    "build:schema": "tsx scripts/build-schema.ts",
    "build:all": "nr build && nr build:schema && nr build:copy-shiki-assets",
    "dev": "nuxi dev",
    "typecheck": "nuxi typecheck",
    "start": "node .output/server/index.mjs",
    "nuxi": "nuxi",
    "lint:js": "eslint .",
    "lint:js:fix": "nr lint:js --fix",
    "lint:css": "stylelint src/**/*.{html,vue,css,scss}",
    "lint:css:fix": "nr lint:css --fix",
    "lint": "nr lint:js && nr lint:css",
    "lint:fix": "nr lint:js:fix && nr lint:css:fix",
    "postinstall": "nuxi prepare",
    "prepare": "husky install",
    "commit": "mcz"
  },
  "devDependencies": {
    "@antfu/ni": "^0.18.8",
    "@dolan-x/markdown-renderer": "^0.0.42",
    "@dolan-x/shared": "^0.0.42",
    "@giscus/vue": "^2.2.6",
    "@iconify-json/carbon": "^1.1.11",
    "@iconify-json/zondicons": "^1.1.3",
    "@kevinmarrec/nuxt-pwa": "^0.13.0",
    "@mini-cz/config-default": "^0.1.2",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/critters": "^0.4.0",
    "@nuxtjs/google-fonts": "3.0.0-1",
    "@nuxtjs/i18n": "8.0.0-beta.7",
    "@pinia/nuxt": "^0.4.6",
    "@so1ve/eslint-config": "^0.36.0",
    "@so1ve/rehype-shiki": "2.2.1-fix.1",
    "@so1ve/stylelint-config": "^0.3.0",
    "@types/fs-extra": "^9.0.13",
    "@vueuse/nuxt": "^9.9.0",
    "@waline/client": "^2.14.3",
    "defu": "^6.1.1",
    "discuss": "^1.2.1",
    "eslint": "^8.30.0",
    "fs-extra": "^11.1.0",
    "gitalk": "^1.8.0",
    "husky": "^8.0.2",
    "mini-cz": "^0.1.2",
    "nuxt": "3.0.0",
    "nuxt-purgecss": "^2.0.0",
    "pinia": "^2.0.28",
    "qrcode.vue": "^3.3.3",
    "sass": "^1.57.1",
    "shiki-es": "^0.1.2",
    "stylelint": "^14.16.0",
    "ts-json-schema-generator": "^1.2.0",
    "tsx": "^3.12.1",
    "twikoo": "^1.6.8",
    "typescript": "^4.9.4",
    "unplugin-icons": "^0.14.15",
    "vite-plugin-toml": "^0.5.0",
    "vue-utterances": "^0.3.4"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "*"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "unplugin",
        "vite",
        "vue",
        "webpack",
        "postcss",
        "rollup",
        "unified",
        "shiki",
        "react",
        "react-dom"
      ]
    },
    "overrides": {
      "nitropack": "npm:@so1ve/nitropack@1.0.0-feat.1"
    }
  }
}
