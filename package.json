    "@waline/client": "^2.15.5",
    "defu": "^6.1.2",
    "discuss": "^1.2.4",
    "eslint": "^8.41.0",
    "fs-extra": "^11.1.1",
    "gitalk": "^1.8.0",
    "mini-cz": "^0.8.0",
    "nuxt": "3.6.0",
    "nuxt-purgecss": "^2.0.0",
    "pinia": "^2.1.3",
    "qrcode.vue": "^3.4.0",
    "sass": "^1.62.1",
    "shiki-es": "^0.2.0",
    "stylelint": "^15.6.2",
    "ts-json-schema-generator": "^1.2.0",
    "tsx": "^3.12.7",
    "twikoo": "^1.6.16",
    "typescript": "^5.0.4",
    "unplugin-icons": "^0.16.1",
    "vite-plugin-toml": "^0.5.0",
    "vue-tsc": "^1.6.5",
    "vue-utterances": "^0.3.4"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "*"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "unplugin",
        "vite",
        "vue",
        "webpack",
        "postcss",
        "rollup",
        "unified",
        "shiki",
