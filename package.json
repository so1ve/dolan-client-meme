    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/robots": "^3.0.0",
    "@pinia/nuxt": "^0.4.6",
    "@so1ve/eslint-config": "^0.42.2",
    "@so1ve/rehype-shiki": "2.2.1-fix.1",
